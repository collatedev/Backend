{
    "types": {
        "request": {
            "access_token": {
                "type": "string",
                "required": false
            },
	        "refresh_token": {
                "type": "any",
                "required": false
            },
            "expires_in": {
                "type": "number",
                "required": false
            },
            "token_type": {
                "type": "string",
                "required": false
            },
	        "scope": {
                "type": "array[string]",
                "required": false
            },
	        "error": {
                "type": "string",
                "required": false
            },
	        "status": {
                "type": "number",
                "required": false
            },
	        "message": {
                "type": "string",
                "required": false
            }
        }
    }
}