{
    "types": {
        "request": {
            "headers": {
                "type": "any",
                "required": true
            },
            "query": {
                "type": "Query",
                "required": true
            }
        },
        "Query": {
            "data": {
                "type": "array[User]",
                "required": true
            }
        },
        "User": {
            "id": {
                "type": "number",
                "required": true
            },
            "login": {
                "type": "string",
                "required": true
            },
            "display_name": {
                "type": "string",
                "required": true
            },
            "type": {
                "type": "string",
                "required": true
            },
            "broadcaster_type": {
                "type": "string",
                "required": true
            },
            "description": {
                "type": "string",
                "required": true
            },
            "profile_image_url": {
                "type": "string",
                "required": true,
                "isURL": true
            },
            "offline_image_url": {
                "type": "string",
                "required": true,
                "isURL": true
            },
            "view_count": {
                "type": "number",
                "required": true
            },
            "email": {
                "type": "string",
                "required": true
            }
        }
    }
}